{
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": "airbnb-base",
  "plugins": [
    "import"
  ],
  "rules": {
    "indent": 0,
    "no-plusplus": 0,
    "quotes": [
      2,
      "single"
    ],
    "no-console": 0,
    "no-unused-vars": 0,
    "semi": [
      2,
      "always"
    ],
    "eqeqeq": 2,
    "new-cap": 0
  }
}
